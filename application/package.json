    "@devexpress/dx-react-core": "^2.7.6",
    "@material-ui/core": "^4.11.4",
    "axios": "^0.18.1",
    "bcrypt": "^5.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "webpack": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
